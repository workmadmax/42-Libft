#----------------------VARIABLE---------------------------------|

NAME = libft.a

CC = gcc

CFLAGS = -Wall -Werror -Wextra

MAIN = main.c libft.h libft.a

RM = rm -f

LINK = ar rc

#----------------------SOURCES FILES----------------------

SRC_CHECK = ft_isalnum.c\
	  		ft_isalpha.c\
			ft_isdigit.c\
			ft_isascii.c\
	  		ft_isprint.c

SRC_CONVERT = ft_atoi.c\
			  ft_itoa.c\
			  ft_tolower.c\
			  ft_toupper.c

SRC_FD = ft_putchar_fd.c\
		 ft_putnbr_fd.c\
		 ft_putendl_fd.c\
		 ft_putstr_fd.c

SRC_MEM = ft_bzero.c\
		  ft_calloc.c\
		  ft_memchr.c\
		  ft_memcmp.c\
		  ft_memcpy.c\
		  ft_memmove.c\
		  ft_memset.c

SRC_STR = ft_strchr.c\
		  ft_strdup.c\
		  ft_striteri.c\
		  ft_strlcat.c\
		  ft_strlcpy.c\
		  ft_strlen.c\
		  ft_strncmp.c\
		  ft_strnstr.c\
		  ft_strrchr.c\
		  ft_substr.c\
		  ft_strtrim.c\
		  ft_split.c\
		  ft_strjoin.c\
		  ft_strmapi.c

ALL_SRC = $(SRC_CHECK)\
		  $(SRC_CONVERT)\
		  $(SRC_FD)\
		  $(SRC_MEM)\
		  $(SRC_STR)

OBJS = $(ALL_SRC:.c=.o)\

#----------------------RULES----------------------

